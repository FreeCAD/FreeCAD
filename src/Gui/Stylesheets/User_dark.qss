/*navgation src/Mod/Tux/NavigationIndicatorGui.py */
Gui--NavigationIndicatorGui--BlenderNavigationStyle {
  qproperty-icon: url(:/icons/icons/NavigationBlender_light.svg);
}

Gui--MDIView QFrame{
  border-top: 3px solid qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(255, 255, 255, 0.05), stop:1 transparent);
  border-left: 3px solid qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(255, 255, 255, 0.05) stop:1 transparent);
  border-right: 3px solid qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 transparent, stop:1 rgba(255, 255, 255, 0.05));
}

Gui--MDIView QLabel{
  border: 0px;
  background-color: transparent;
}

Gui--MDIView QScrollArea{
  border: 1px solid rgba(0, 0, 0, 0.8);
  background-color: transparent;/* Added to maintain consistency */
}

