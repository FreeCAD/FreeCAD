  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 @ThemeGradientColor1,stop:1 @ThemeGradientColor2  );
}

QToolBox::tab:selected:disabled {
  background-color: rgba(100, 100, 100, 0.2);
  border-bottom: 0px solid @ThemeBackground;
  color: rgba(175, 175, 175, 0.8);
}

QToolBox::tab:!selected {
}

QToolBox::tab:!selected:disabled {
  background-color: rgba(100, 100, 100, 0.2);
}

QToolBox::tab:hover {
  background-color: @ThemeBackground;
}

QToolBox > QScrollArea > QWidget > QWidget {
  padding: 0px;
  border: 0px;
  background-color: @ThemeBackground;
  border-radius: 4px;
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe
https://doc.qt.io/qt-5/qframe.html#-prop
https://doc.qt.io/qt-5/qframe.html#details
https://stackoverflow.com/questions/14581498/qt-stylesheet-for-hline-vline-color

--------------------------------------------------------------------------- */
/* (dot) .QFrame  fix #141, #126, #123 */
.QFrame {
  border-radius: 0px;
  border: 0px solid rgba(0, 0, 0, 0.5);
  background-color: @ThemeBackground;
}

.QFrame[frameShape="0"] {
  border-radius: 3px;
  border: 1px solid rgba(0, 0, 0, 0.8);
}

.QFrame[frameShape="4"] {
  max-height: 1px;
  border: none;
  background-color: @ThemeBackground;
}

.QFrame[frameShape="5"] {
  max-width: 1px;
  border: none;
  background-color: @ThemeBackground;
}

/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

---------------------------------------------------------------------------*/
QSplitterHandle:hover { /* to fix bug about hovering on splitters https://bugreports.qt.io/browse/qTBUG-13768 */
}

QSplitter::handle {
  margin: 0px 0px;
  padding: 0px;
}

QSplitter::handle:horizontal {
  background-image: none;
  background-position: center center;
  background-repeat: none;
  margin: 2px 2px 2px 2px;
  width: 1px;
}

QSplitter::handle:vertical {
  background-image: none;
  background-position: center center;
  background-repeat: none;
  margin: 2px 2px 2px 2px;
  height: 1px;
}

QSplitter::handle:vertical:hover {
  image: url(qss:images_classic/splitter_horizontal-lightgray.svg);
}

QSplitter::handle:horizontal:hover {
  image: url(qss:images_classic/splitter_vertical-lightgray.svg);
}

/* QDateEdit, QDateTimeEdit -----------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit, QDateTimeEdit {
  selection-background-color: @ThemeAccentColor1;
  border-style: solid;
  border: 1px solid rgba(0, 0, 0, 0.8);
  border-radius: 3px;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px;
  min-width: 10px;
}

QDateEdit:on, QDateTimeEdit:on {
  selection-background-color: @ThemeAccentColor1;
}

QDateEdit::drop-down, QDateTimeEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 12px;
  border-left: 1px solid rgba(0, 0, 0, 0.5);
}

QDateEdit::down-arrow, QDateTimeEdit::down-arrow {
  image: url(qss:images_classic/arrow-down-lightgray.svg);
  height: 8px;
  width: 8px;
}

QDateEdit::down-arrow:on, QDateEdit::down-arrow:hover, QDateEdit::down-arrow:focus, QDateTimeEdit::down-arrow:on, QDateTimeEdit::down-arrow:hover, QDateTimeEdit::down-arrow:focus {
  image: url(qss:images_classic/arrow-down-white.svg);
}

QDateEdit QAbstractItemView, QDateTimeEdit QAbstractItemView {
  background-color: @ThemeBackground;
  border-radius: 3px;
  border: 1px solid rgba(0, 0, 0, 0.8);
  selection-background-color: @ThemeAccentColor1;
}

/* QAbstractView ----------------------------------------------------------

--------------------------------------------------------------------------- */
QAbstractView:hover {
  border: 1px solid @ThemeAccentColor1;
}

QAbstractView:selected {
  background-color: @ThemeAccentColor1;
}

/* PlotWidget -------------------------------------------------------------

--------------------------------------------------------------------------- */
PlotWidget {
  padding: 0px;
  color: rgba(175, 175, 175, 0.8);
}

