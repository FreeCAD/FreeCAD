/* QMainWindow ------------------------------------------------------------

This adjusts the splitter in the dock widget, not qsplitter
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmainwindow

--------------------------------------------------------------------------- */
QMainWindow::separator {
  border: 0px solid rgba(0, 0, 0, 0.5);
  spacing: 0px;
  padding: 1px;
}

QMainWindow::separator:vertical:hover {
  image: url(qss:images_classic/splitter_vertical-lightgray.svg);
}

QMainWindow::separator:horizontal:hover {
  image: url(qss:images_classic/splitter_horizontal-lightgray.svg);
}

QMainWindow::separator:horizontal {
  width: 4px;
  margin-top: 0.1px;
  margin-bottom: 0.1px;
}

QMainWindow::separator:vertical {
  height: 4px;
  margin-left: 0.1px;
  margin-right: 0.1px;
}

/* QToolTip ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtooltip

--------------------------------------------------------------------------- */
QToolTip {
  background-color: @themeBackground;
  /* If you remove the border property, background stops working on Windows */
  border: 1.5px solid rgba(0, 0, 0, 0.8);
  font-weight:bolder;
  padding: 0 2px 0 2px;
  border-radius: 5px;
}

/* QStatusBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qstatusbar

--------------------------------------------------------------------------- */
QStatusBar {
  border: 0px solid rgba(255, 255, 255, 0.08) ;
  background-color: @ThemeBackground;
}

QStatusBar::item {
  border: none;
}

QStatusBar QToolTip {
  background-color: @ThemeBackground;
  border: 1px solid rgba(0, 0, 0, 0.8);
  /* Remove padding, for fix combo box tooltip */
  padding: 0px;
  /* Reducing transparency to read better */
  opacity: 230;
}

QStatusBar QLabel {
  background-color: transparent;
}

/* QCheckBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcheckbox

--------------------------------------------------------------------------- */
QCheckBox {
  color: white;
  padding: 1px;
  outline: none;
  border: 1px solid transparent;
  background-color: transparent;
  spacing: 8px;
}

QCheckBox:focus {
  border: 1px solid @ThemeAccentColor2;
}

QCheckBox QWidget:disabled {
  color: rgba(175, 175, 175, 0.8);
}

QCheckBox::indicator {
  color: white;
  background-color:  rgba(10, 10, 10, 0.65);
  width: 12px;
  height: 12px;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:unchecked {
  background-color:  rgba(10, 10, 10, 0.65);
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:unchecked:hover, QCheckBox::indicator:unchecked:focus, QCheckBox::indicator:unchecked:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:unchecked:disabled {
  background-color: rgba(255, 255, 255, 0.05);
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:checked {
  image:url(qss:images_classic/check-mark-white.svg);
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:checked:hover, QCheckBox::indicator:checked:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QCheckBox::indicator:checked:disabled {
  background-color: @ThemeBackground;
  border: 1px solid rgba(255, 255, 255, 0.8);
  image: url(qss:images_classic/check-mark-lightgray.svg);
}

QCheckBox::indicator:indeterminate {
  background-color: @ThemeBackground;
  border: 1px solid rgba(255, 255, 255, 0.8);
  image: url(qss:images_classic/indeterminate-mark-white.svg);
}

QCheckBox::indicator:indeterminate:disabled {
  background-color: @ThemeBackground;
  border: 1px solid rgba(255, 255, 255, 0.8);
  image: url(qss:images_classic/indeterminate-mark-lightgray.svg);
}

QCheckBox::indicator:indeterminate:hover, QCheckBox::indicator:indeterminate:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

/* QGroupBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qgroupbox

--------------------------------------------------------------------------- */
QGroupBox {
  background-color: @ThemeGradientColor1;
  border-radius: 5px;
  border: 1px solid rgba(255, 255, 255, 0.05);
  margin: 18px 5px 2px 5px;/*leave space at the top for the title */
}

QGroupBox::title {
  top: -18px;
  left: 0px;
}

QGroupBox::indicator {
  background-color: @ThemeBackground;
  width: 12px;
  height: 12px;
  margin: 1px;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QGroupBox::indicator:unchecked {
  background-color: @ThemeBackground;
}

QGroupBox::indicator:unchecked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QGroupBox::indicator:unchecked:focus {
  border: 1px solid @ThemeAccentColor2;
}

QGroupBox::indicator:unchecked:disabled {
  background-color: rgba(50, 50, 50, 0.79);
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QGroupBox::indicator:checked {
  background-color: @ThemeBackground;
  border: 1px solid rgba(255, 255, 255, 0.8);
  image:url(qss:images_classic/check-mark-white.svg);
}

QGroupBox::indicator:checked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(255, 255, 255, 0.8);
}

QGroupBox::indicator:checked:focus {
  border: 1px solid @ThemeAccentColor2;
}

QGroupBox::indicator:checked:disabled {
  background-color: rgba(50, 50, 50, 0.79);
  image:url(qss:images_classic/check-mark-lightgray.svg);
  border: 1px solid rgba(255, 255, 255, 0.8);
}

/* QRadioButton -----------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qradiobutton

--------------------------------------------------------------------------- */
QRadioButton {
  background-color: transparent;
  color: white;
  border: none;
  spacing: 8px;
  outline: none;
}

QRadioButton:focus {
  border: 1px solid @ThemeAccentColor2;
}

QRadioButton:disabled {
  background-color: rgba(100, 100, 100, 0.2);
}

QRadioButton QWidget {
  background-color: transparent;
  color: white;
  spacing: 0px;
  padding: 0px;
  outline: none;
  border: none;
}

QRadioButton::indicator {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/radio-unchecked-white.svg);
}

QRadioButton::indicator:unchecked {}

QRadioButton::indicator:unchecked:hover, QRadioButton::indicator:unchecked:pressed {
}

QRadioButton::indicator:unchecked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/radio-unchecked-lightgray.svg);
}

QRadioButton::indicator:checked {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/radio-checked-white.svg);
}

QRadioButton::indicator:checked:hover, QRadioButton::indicator:checked:pressed {
}

QRadioButton::indicator:checked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/radio-checked-lightgray.svg);
}

/* QMenuBar ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenubar

--------------------------------------------------------------------------- */
QMenuBar {
  background-color: @ThemeMenuColor;
  selection-background-color: @ThemeAccentColor1;
}

QMenuBar:disabled,
QMenuBar::item:disabled{
  color: rgba(175, 175, 175, 0.8);
}

QMenuBar:focus {
  border: 1px solid @ThemeAccentColor2;
}

QMenuBar::item {
  background-color: transparent;
}

QMenuBar::item:selected {
  background-color: transparent;
  border: 0px solid rgba(0, 0, 0, 0.5);
  background-color: @ThemeAccentColor1;
}

QMenuBar::item:pressed {
  border: 0px solid rgba(0, 0, 0, 0.5);
  background-color: @ThemeAccentColor1;
}

/* QMenu ------------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu

--------------------------------------------------------------------------- */
QMenu {
  border: 1px solid rgba(0, 0, 0, 0.8);
  margin: 0px;
  background-color: @ThemeMenuColor;
  selection-background-color: @ThemeAccentColor1;
}

QMenu::item:disabled {
  color: rgba(175, 175, 175, 0.8);
}

QMenu::separator {
  height: 2px;
  background-color: @ThemeBackground;
  margin-left: 10%;
  margin-right: 10%;
}

QMenu::item {
  background-color: transparent;
  padding-left: 5px;
  padding-top: 4px;
  padding-bottom: 4px;
  padding-right: 50px;
  /* Reserve space for selection border */
}

QMenu::item:selected {
  background-color: @ThemeAccentColor1;
}

QMenu::item:pressed {
  background-color: @ThemeAccentColor1;
}

QMenu::icon {
  margin-left: 5px;
  width: 14px;
  height: 14px;
}

QMenu::indicator {
  margin-left: 2px;
  margin-right: 2px;
  padding: 0px;
  width: 12px;
  height: 12px;
  /* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
  /* exclusive indicator = radio button style indicator (see QActionGroup::setExclusive) */
}

QMenu::icon:checked { /* appearance of a 'checked' icon */
  background-color: @ThemeAccentColor2;
  margin-left: -5px;
  border: 5px solid @ThemeAccentColor2;
  position: absolute;
  border-radius: 0px;
}

QMenu::indicator:non-exclusive:unchecked {
  image: url(qss:images_classic/checkbox-empty-white.svg);
}

QMenu::indicator:non-exclusive:unchecked:hover, QMenu::indicator:non-exclusive:unchecked:focus, QMenu::indicator:non-exclusive:unchecked:pressed {
  border: none;
  background-color: @ThemeAccentColor1;
}

QMenu::indicator:non-exclusive:unchecked:disabled {
  image: url(qss:images_classic/checkbox-empty-lightgray.svg);
}

QMenu::indicator:non-exclusive:checked {
  image:url(qss:images_classic/checkbox-checked-white.svg);
}

QMenu::indicator:non-exclusive:checked:hover, QMenu::indicator:non-exclusive:checked:focus, QMenu::indicator:non-exclusive:checked:pressed {
  border: none;
  background-color: @ThemeAccentColor1;
}

QMenu::indicator:non-exclusive:checked:disabled {
  image:url(qss:images_classic/checkbox-checked-lightgray.svg);
}

QMenu::indicator:non-exclusive:indeterminate {
  image: url(qss:images_classic/checkbox-indeterminate-white.svg);
}

QMenu::indicator:non-exclusive:indeterminate:disabled {
  image: url(qss:images_classic/checkbox-indeterminate-white.svg);
}

QMenu::indicator:non-exclusive:indeterminate:focus, QMenu::indicator:non-exclusive:indeterminate:hover, QMenu::indicator:non-exclusive:indeterminate:pressed {
  image: url(qss:images_classic/checkbox-indeterminate-white.svg);
}

QMenu::indicator:exclusive:unchecked {
  image: url(qss:images_classic/transparent.svg);
}

QMenu::indicator:exclusive:unchecked:hover, QMenu::indicator:exclusive:unchecked:focus, QMenu::indicator:exclusive:unchecked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
  image: url(qss:images_classic/checkbox-empty-white.svg);
}

QMenu::indicator:exclusive:unchecked:disabled {
  image: url(qss:images_classic/checkbox-empty-white.svg);
}

QMenu::indicator:exclusive:checked {
  border: none;
  outline: none;
  image: url(qss:images_classic/radio-checked-white.svg);
}

QMenu::indicator:exclusive:checked:hover, QMenu::indicator:exclusive:checked:focus, QMenu::indicator:exclusive:checked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
  image: url(qss:images_classic/checkbox-empty-white.svg);
}

QMenu::indicator:exclusive:checked:disabled {
  outline: none;
  image: url(qss:images_classic/radio-checked-white.svg);
}

QMenu::right-arrow {
  margin: 5px;
  padding-left: 12px;
  image:url(qss:images_classic/arrow-right-white.svg);
  height: 8px;
  width: 8px;
  background-color: transparent;
}

/* QAbstractItemView ------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcombobox

--------------------------------------------------------------------------- */
QAbstractItemView {
  alternate-background-color: rgba(255,255,255, 0.1);
  border: 0px solid rgba(255, 0, 0, 0.8);
}

QAbstractItemView QLineEdit {
  padding: 0 2px;
}

/* QAbstractScrollArea ----------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qabstractscrollarea

--------------------------------------------------------------------------- */
QAbstractScrollArea {
  background-color: transparent;
  padding: 0px;
}

QAbstractScrollArea:disabled {
  background-color: rgba(255, 255, 255, 0.08);
}

/* QScrollArea ------------------------------------------------------------

---------------------------------------------------------------------------
text input field disabled!!!!*/
QScrollArea QWidget:disabled {
  color: rgba(175, 175, 175, 0.8);
}

/* QScrollBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qscrollbar

--------------------------------------------------------------------------- */

QScrollBar:horizontal {
  border-right: 14px solid qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 rgba(0, 0, 0, 0.6), stop:0.1 @ThemeGradientColor1 , stop:0.9 @ThemeGradientColor1 , stop:1 rgba(0, 0, 0, 0.6));
  border-left: 14px solid qlineargradient(x1:1, y1:0, x2:0, y2:0, stop:0 rgba(0, 0, 0, 0.6), stop:0.1 @ThemeGradientColor1 , stop:0.9 @ThemeGradientColor1 , stop:1 rgba(0, 0, 0, 0.6));
  border-top: 1px solid rgba(10, 10, 10, 0.8);
  border-bottom: 1px solid rgba(10, 10, 10, 0.8);
  background-color: rgba(25, 25, 25, 0.9);
}

QScrollBar:vertical {
  border-top: 14px solid qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0 rgba(0, 0, 0, 0.6), stop:0.1 @ThemeGradientColor1 , stop:0.9 @ThemeGradientColor1 , stop:1 rgba(0, 0, 0, 0.6));
  border-bottom: 14px solid qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0, 0, 0, 0.6), stop:0.1 @ThemeGradientColor1 , stop:0.9 @ThemeGradientColor1 , stop:1 rgba(0, 0, 0, 0.6));
  border-left: 1px solid rgba(10, 10, 10, 0.8);
  border-right: 1px solid rgba(10, 10, 10, 0.8);
  background-color: rgba(25, 25, 25, 0.9);
}

QScrollBar::handle:horizontal {
  border-left: 1px solid rgba(0, 0, 0, 0.5);
  border-right: 1px solid rgba(0, 0, 0, 0.5);
  min-width: 20px;
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 @ThemeGradientColor1, stop:1 @ThemeGradientColor2  );
}

QScrollBar::handle:horizontal:hover {
  background-color: @ThemeGradientColor1;
  border: 1px @ThemeGradientColor1;
}

QScrollBar::handle:horizontal:focus {
  border: 1px solid @ThemeAccentColor2;
}

QScrollBar::handle:vertical {
  border-top: 1px solid rgba(0, 0, 0, 0.5);
  border-bottom: 1px solid rgba(0, 0, 0, 0.5);
  min-height: 20px;
  border-radius: 0px;
  background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 @ThemeGradientColor1, stop:1 @ThemeGradientColor2  );
}

QScrollBar::handle:vertical:hover {
  background-color: @ThemeGradientColor1;
  border: 1px @ThemeGradientColor1;
}

QScrollBar::handle:vertical:focus {
  border: 1px solid @ThemeAccentColor2;
}

QScrollBar::add-line:horizontal {
  margin: 0px 2px 0px 2px;
  border-image: url(qss:images_classic/arrow-right-lightgray.svg);
  height: 10px;
  width: 10px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover, QScrollBar::add-line:horizontal:on {
  border-image: url(qss:images_classic/arrow-right-white.svg);
}

QScrollBar::add-line:vertical {
  margin: 2px 0px 2px 0px;
  border-image: url(qss:images_classic/arrow-down-lightgray.svg);
  height: 10px;
  width: 10px;
  subcontrol-position: bottom;
  subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover, QScrollBar::add-line:vertical:on {
  image: url(qss:images_classic/arrow-down-white.svg);
}

QScrollBar::sub-line:horizontal {
  margin: 0px 2px 0px 2px;
  border-image: url(qss:images_classic/arrow-left-lightgray.svg);
  height: 10px;
  width: 10px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover, QScrollBar::sub-line:horizontal:on {
  border-image: url(qss:images_classic/arrow-left-white.svg);
}

QScrollBar::sub-line:vertical {
  margin: 2px 0px 2px 0px;
  border-image: url(qss:images_classic/arrow-up-lightgray.svg);
  height: 10px;
  width: 10px;
  subcontrol-position: top;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover, QScrollBar::sub-line:vertical:on {
  border-image: url(qss:images_classic/arrow-up-white.svg);
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
  background-color: none;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
  background-color: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
  background-color: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
  background-color: none;
}

