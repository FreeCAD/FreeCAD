text-align: left;
font-weight: bold;
border: none;
margin: 0px;
padding: 0px;
}

QSint--ActionGroup QFrame[class="header"] QLabel {
background-color: transparent;
background-image: url(qss:images_dark-light/down_arrow_dark.svg);
