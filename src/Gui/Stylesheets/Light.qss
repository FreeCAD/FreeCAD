}

QMenuBar::item {
  background: transparent;
    /*padding-left:5px;
  padding-right: 5px;
  padding-bottom: 1px;
  padding-top: 1px;*/
}

QMenuBar::item:selected {
  background: transparent;
  border: 0px solid #C1C1C1;
