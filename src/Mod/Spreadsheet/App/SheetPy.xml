      <Documentation>
        <UserDocu>Merge given cell area into one cell</UserDocu>
      </Documentation>
    </Methode>
    <Methode Name="splitCell">
      <Documentation>
        <UserDocu>Split a previously merged cell</UserDocu>
      </Documentation>
    </Methode>
