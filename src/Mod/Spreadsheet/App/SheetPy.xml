      <Documentation>
        <UserDocu>Set given spreadsheet column to given width</UserDocu>
      </Documentation>
    </Methode>
    <Methode Name="getColumnWidth">
      <Documentation>
        <UserDocu>Get given spreadsheet column width</UserDocu>
      </Documentation>
    </Methode>
