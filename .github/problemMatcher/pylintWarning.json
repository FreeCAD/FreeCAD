{
  "problemMatcher": [
    {
      "owner": "pylint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+?):([0-9]+):([0-9]+): ([^E|F][0-9]+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
